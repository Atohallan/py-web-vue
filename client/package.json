{
  "name": "py-web-vue",
  "version": "0.0.1",
  "description": "Generic client application to work with a PyWebVue server",
  "keywords": [
    "Kitware",
    "Python Server",
    "VTK",
    "ParaView"
  ],
  "bugs": {
    "url": "https://github.com/Kitware/py-web-vue/issues"
  },
  "license": "BSD-3-Clause",
  "funding": {
    "type": "Kitware Support",
    "url": "https://www.kitware.com/what-we-offer/#support"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "commit": "git cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@mdi/font": "6.4.95",
    "core-js": "^3.19.0",
    "lodash": "^4.17.21",
    "semantic-release-pypi": "^2.3.0",
    "typeface-roboto": "^1.1.13",
    "v-runtime-template": "^1.10.0",
    "vtk.js": "20.2.3",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vue-vtk-js": "1.2.0",
    "vuetify": "2.5.10",
    "vuex": "^3.4.0",
    "wslink": "1.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "commitizen": "4.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "2.4.1",
    "semantic-release": "17.4.2",
    "shader-loader": "1.3.1",
    "vue-template-compiler": "^2.6.14"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}",
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      [
        "semantic-release-pypi",
        {
          "setupPy": "../python/setup.py",
          "distDir": "../dist"
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
